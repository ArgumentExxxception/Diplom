@inherits MudComponentBase

<MudDialog>
    <DialogContent>
        <MudText>@ContentText</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="InsertIntoNew" Color="Color.Primary">Вставить в новую</MudButton>
        <MudButton OnClick="InsertIntoCurrent" Color="Color.Secondary">Вставить в текущую</MudButton>
    </DialogActions>
</MudDialog>